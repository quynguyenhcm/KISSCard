<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.qrolling.kisscard">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.KissCardMainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.ResultActivity"
            android:label="@string/title_activity_result" />
        <activity
            android:name=".activities.CardListActivity"
            android:label="@string/title_activity_main" />
        <activity
            android:name=".activities.CardView"
            android:label="@string/title_activity_card_view" />
        <activity android:name=".activities.AddCardActivity"></activity>
    </application>

</manifest>